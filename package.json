{
  "name": "katsuragi",
  "version": "0.1.0",
  "description": "Text-based UI wireframe generator",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "katsuragi": "./dist/cli.js",
    "ktr": "./dist/cli.js"
  },
  "files": [
    "dist",
    "examples"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "vitest",
    "lint": "biome lint src tests",
    "format": "biome format --write src tests",
    "check": "biome check --write src tests"
  },
  "keywords": [
    "wireframe",
    "ui",
    "svg",
    "png",
    "design"
  ],
  "author": "En-Links LLC",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enlinks-llc/katsuragi.git"
  },
  "homepage": "https://github.com/enlinks-llc/katsuragi#readme",
  "bugs": {
    "url": "https://github.com/enlinks-llc/katsuragi/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "node-html-parser": "^7.0.2",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
